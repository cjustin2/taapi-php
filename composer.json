{
    "name": "tigusigalpa/taapi-php",
    "description": "Modern PHP/Laravel library for taapi.io technical analysis API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Sazonov",
            "email": "sovletig@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tigusigalpa\\TAAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tigusigalpa\\TAAPI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tigusigalpa\\TAAPI\\Laravel\\TAAPIServiceProvider"
            ],
            "aliases": {
                "TAAPI": "Tigusigalpa\\TAAPI\\Laravel\\Facades\\TAAPI"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
